    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "next-themes": "^0.4.6",
    "ogl": "^1.0.11",
    "postprocessing": "^6.38.2",
    "react-day-picker": "^9.13.0",
    "react-hook-form": "^7.69.0",
    "react-resizable-panels": "^2.1.9",
    "recharts": "2.15.4",
    "rough-notation": "^0.5.1",
    "sonner": "^2.0.7",
    "svg-dotted-map": "^2.0.1",
    "tailwind-merge": "^3.4.0",
    "three": "^0.167.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.182.0",
    "eslint": "^9.39.2",
    "next": "16.1.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.53.1"
  }
